{
  "name": "electron",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "build": {
    "appId": "oneoften",
    "files": [
      "./build/**/*",
      "./main.js"
    ],
    "asar": false
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package": "electron-packager ./ app --win --out presenterTool --arch=x64 --app-version 1.3.4 --overwrite --ignore=node_modules",
    "package-build": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1",
    "mime": "^2.4.2",
    "typescript": "^3.4.3"
  }
}
